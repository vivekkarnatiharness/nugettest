<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <PackageId>PasswordBoxMVVM</PackageId>
    <Authors>SingletonSean</Authors>
    <Description>A password box that allows binding.</Description>
    <PackageTags>password box wpf binding mvvm</PackageTags>
    <PackageProjectUrl>https://github.com/SingletonSean/wpf-mvvm-password-box</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SingletonSean/wpf-mvvm-password-box</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

</Project>
